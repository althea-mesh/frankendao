{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "interface-over-type-literal": false,
      "semicolon": [true, "never"],
      "quotemark": [true, "single", "jsx-double"],
      "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
    },
    "rulesDirectory": []
}
